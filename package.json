  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alesgenova/stenciljs-in-vue.git"
  },
  "author": "Alessandro Genova",
  "license": "MIT",
  "keywords": [
    "stenciljs",
    "webcomponents",
    "vue",
    "javascript",
    "typescript"
  ]
}